{
  "name": "myWishUI",
  "version": "1.0.0",
  "description": "",
  "author": "Mauricio Joost.",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "watch":
      "nodemon --config nodemon.json --exec ./node_modules/.bin/ts-node src/server.ts",
    "build": "tsc",
    "scss": "node-sass --watch src/public -o src/public",
    "build-css":
      "node-sass --include-path scss src/public/site.scss src/public/site.css",
    "plop": "plop"
  },
  "dependencies": {
    "@types/cors": "2.8.4",
    "@types/express": "4.16.0",
    "cors": "2.8.4",
    "directory-tree": "^2.2.3",
    "express": "4.16.4",
    "typescript": "3.1.3"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "plop": "^2.4.0",
    "ts-node": "^7.0.1"
  }
}
